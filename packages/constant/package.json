{
  "name": "constant",
  "version": "1.0.0",
  "author": "Dmitry Paskhin <d.pasxin@gmail.com>",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build:prod": "npm run clean:dist && ttsc --project tsconfig.build.json && rollup --input ./dist/index.js --file ./dist/index.cjs --format cjs",
    "build:start": "exit 0",
    "build:watch": "exit 0",
    "clean:dist": "rimraf './dist'",
    "clean:all": "npm run clean:dist && rimraf './node_modules'",
    "check:lint": "tsc --noEmit && eslint \"{**/*,*}.{ts,js,json}\"",
    "check:lint:fix": "tsc --noEmit && eslint \"{**/*,*}.{ts,js,json}\" --fix",
    "check:test": "exit 0",
    "check:all": "npm run check:lint && npm run check:test"
  },
  "dependencies": {
    "ttypescript": "^1.5.15",
    "rollup": "^3.20.2"
  }
}
